@model SDMS.DTO.DTO.NewsDTO
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>公告</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
    <script src="~/js/jquery-1.10.2.min.js"></script>
</head>
<body class="bgcolor">
    <div class="notice">
        <h1 class="notice-title">@Model.Title</h1>
        <div class="notice-con">@Html.Raw(Model.Content)</div>
        <div class="notice-bt">
            <a href="#" class="bd-browse">@Model.Click</a>
        </div>
    </div>
    <div class="form-btn">
        <a href="/home/message/@Model.Id">留言反馈</a>
    </div>
</body>
</html>
