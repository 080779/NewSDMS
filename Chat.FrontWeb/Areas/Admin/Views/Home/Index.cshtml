@model SDMS.Web.Areas.Admin.Models.IndexViewModel
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>主页</title>
    <link href="~/css/admin/Base.css" rel="stylesheet" />
    <link href="~/css/admin/themes/icon.css" rel="stylesheet" />
    <link href="~/css/admin/themes/default/easyui.css" rel="stylesheet" />
    <script src="~/js/admin/jquery.js"></script>
    <script src="~/js/admin/jquery.easyui.min.js"></script>
    <script src="~/js/admin/Common.js"></script>
    <script src="~/js/admin/main-layout.js"></script>
    <script src="~/js/admin/My97DatePicker/WdatePicker.js"></script>
</head>
<body class="easyui-layout">
    <!--头部s-->
    <div region="north" style="overflow:hidden;">
        <div class="index_logo">
            <div class="index_logo_top"></div>
            <div class="index_logo_bottom">
                <p><a href="/admin/manager/logout">[退出系统]</a></p>
                <p>欢迎 <span>@Model.Name</span>登录后台管理</p>
            </div>
        </div>
    </div>
    <!--头部e-->
    <!--导航s-->
    <div region="west" split="true" title="系统菜单" style="width: 200px; padding: 1px;
        overflow: hidden;">
        <div class="easyui-accordion" fit="true" border="false">
            @foreach (var list in Model.MenuList)
            {
                <div title="@list.Parent.MenuName" style="padding: 10px; overflow: auto;">
                    @foreach (var modelList in list.Child)
                    {
                        <p>
                            <a class="h30" onclick="console.log('@(modelList.URL)/?time='+new Date().getTime());addTab('@modelList.MenuName','@(modelList.URL)/?time='+new Date().getTime(),@modelList.Id)">@modelList.MenuName</a>
                        </p>
                    }
                </div>
            }
        </div>
    </div>
    <!--导航e-->
    <!--中间内容s-->
    <div id="main-panel" region="center" style="background: #eee; overflow-y:hidden">
        <div id="tabs" class="easyui-tabs" fit="true" border="false">
            <div title="首页" style="padding:20px;overflow:hidden; color:red; ">
                <iframe scrolling="auto" id="ifr我的工作台" frameborder="0" src="/admin/home/zhuye" style="width: 100%; height: 100%;"></iframe>
            </div>
        </div>
    </div>
</body>
</html>
